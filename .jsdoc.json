{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "ramdu.svg", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "referenceTitle": "ramdu",
    "collapse": true,
    "disableSort": false,
    "resources": {
      "Github": "https://github.com/jmlweb/ramdu"
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./jsdoc-template"
  }
}
