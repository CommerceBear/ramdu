{
  "name": "ramdu",
  "version": "0.0.8",
  "description": "Small utils for ramda",
  "keywords": [
    "fp",
    "javascript",
    "functional programming",
    "ramda",
    "utility belt"
  ],
  "main": "cjs/index.js",
  "module": "es/index.js",
  "author": "JoseÃÅ Manuel Lucas <josemanuel@jmlweb.es>",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">= 3.10.10"
  },
  "scripts": {
    "test": "nyc --check-coverage --lines 95 --functions 95 --branches 95 --reporter=lcov --reporter=text-summary --reporter=html mocha --require @babel/register --timeout=3000 src/**/*.spec.js",
    "test:dev": "mocha --watch --require @babel/register src/**/*.spec.js",
    "clean": "rimraf ./cjs ./es ./iife",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "build:dev": "rollup -c -w",
    "doc": "jsdoc --configure .jsdoc.json --verbose",
    "predoc:serve": "npm run doc",
    "doc:serve": "serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run doc && git add . --all"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^2.4.0",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "ramda": "^0.26.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "serve": "^11.0.1"
  },
  "peerDependencies": {
    "ramda": "^0.26.1"
  }
}
