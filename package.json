{
  "name": "@commercebear/ramdu",
  "version": "0.0.17",
  "description": "Small utils for ramda",
  "keywords": [
    "fp",
    "javascript",
    "functional programming",
    "ramda",
    "utility belt"
  ],
  "main": "cjs/index.js",
  "module": "es/index.js",
  "author": "JoseÃÅ Manuel Lucas <josemanuel@jmlweb.es>",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">= 3.10.10"
  },
  "scripts": {
    "test": "nyc --check-coverage --lines 95 --functions 95 --branches 95 --reporter=lcov --reporter=text-summary --reporter=html mocha --require @babel/register --timeout=3000 src/**/*.spec.js",
    "test:dev": "mocha --watch --require @babel/register src/**/*.spec.js",
    "clean": "rimraf ./cjs ./es ./iife",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "build:dev": "rollup -c -w",
    "doc": "jsdoc --configure .jsdoc.json --verbose",
    "predoc:serve": "npm run doc",
    "doc:serve": "serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run doc && git add . --all"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "assert": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^4.2.3",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "ramda": "^0.27.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "serve": "^11.2.0"
  },
  "peerDependencies": {
    "ramda": ">=0.26.1"
  }
}
